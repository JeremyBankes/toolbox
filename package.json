{
    "name": "@jeremy-bankes/toolbox",
    "version": "1.8.18",
    "description": "A utility library delivering many quality of life improvements to both browser and node environments.",
    "types": "build/types",
    "exports": {
        "./test": "./build/index.js",
        "./client": "./build/client/index.js",
        "./server": "./build/server/index.js"
    },
    "scripts": {
        "setup": "npm install",
        "clean": "rmdir /Q /S build 2>null",
        "build": "npx webpack",
        "build:clean": "npm run clean && npm run build",
        "build:watch": "npx webpack --watch",
        "publish": "npm run build:clean && npm publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/JeremyBankes/toolbox.git"
    },
    "keywords": [
        "Tools",
        "JavaScript",
        "TypeScript",
        "Utility"
    ],
    "author": "Jeremy Bankes",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/JeremyBankes/toolbox/issues"
    },
    "homepage": "https://github.com/JeremyBankes/toolbox#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@types/node": "^20.2.4",
        "babel-loader": "^9.1.2",
        "nodemon": "^2.0.19",
        "ts-loader": "^9.4.3",
        "typescript": "^4.8.3",
        "webpack-cli": "^5.1.1"
    }
}